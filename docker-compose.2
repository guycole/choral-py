version: '2'

services:
  django:
    image: httpd

  postgres:
    image: postgres:9.6
    environment:
      - POSTGRES_PASSWORD=worksux
    volumes:
      - choral-data:/var/lib/postgresql/data

  redis:
    image: redis:latest
    volumes:
      - redisdata:/data
  
volumes:
  choral-data:
  redis-data:
